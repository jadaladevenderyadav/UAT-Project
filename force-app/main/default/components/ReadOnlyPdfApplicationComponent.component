                                <apex:outputText value="{!tenth[f.apiName2]}" rendered="{!f.fieldType2 != 'DATE' && f.fieldType2 != 'DATETIME'}"/>
                                <apex:outputText value="{0, Date, dd-MMM-yyyy}" rendered="{!f.fieldType2 == 'DATE' || f.fieldType2 == 'DATETIME'}">
                                    <apex:param value="{!tenth[f.apiName2]}" />
