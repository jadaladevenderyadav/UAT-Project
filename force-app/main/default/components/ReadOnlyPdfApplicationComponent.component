                            <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                                <apex:outputText value="{!father[f.apiName1]}" />
                            </td>
                            <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                                <b><apex:outputText value="{!f.label2}" /></b>
                            </td>
