            </table>
            
            <apex:outputPanel rendered="{!tenth != null}">
                
                <h1 style = "font-size:18px; margin:5px 0px; padding-left:10px; color:white; background-color:Gray; width:98.7%;"><b>10th Board / Equivalent</b></h1>               
                <table style = "table-layout: fixed; width: 100%; border: 1px solid black;border-collapse: collapse;padding-left: 10px;">
                    <apex:repeat value="{!fieldsInfo['education10th']}"  var="f">
                        <tr style="border:1px solid black;">
                            <td style="width: 25%;word-break: break-all; border: 1px solid black;border-collapse: collapse;padding-left: 10px;">
                                <b><apex:outputText value="{!f.label1}" /></b>
                            </td>
                            <td style="width: 25%;word-break: break-all; border: 1px solid black;border-collapse: collapse;padding-left: 10px;">
                                <apex:outputText value="{!tenth[f.apiName1]}" rendered="{!f.fieldType1 != 'DATE' && f.fieldType1 != 'DATETIME'}"/>
                                <apex:outputText value="{0, Date, dd-MMM-yyyy}" rendered="{!f.fieldType1 == 'DATE' || f.fieldType1 == 'DATETIME'}">
                                    <apex:param value="{!tenth[f.apiName1]}" />
                                </apex:outputText>
                            </td>
                            <td style="width: 25%;word-break: break-all; border: 1px solid black;border-collapse: collapse;padding-left: 10px;">
