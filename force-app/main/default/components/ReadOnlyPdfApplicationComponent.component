<apex:component controller="ReadOnlyApplicationController"> 
    
    <html>
        
        <head>
            <style type="text/CSS">
                <!--    table{
                    table-layout: fixed;
                    width: 100%;
                    }
                    td{
                    width: 25%;
                    word-break: break-all;
                    } -->
            </style>
        </head>
        
        <body style = "font-family:sans-serif;font-size:13px;">
            <div class="header" style="margin:5px; padding-left:10px; padding-top:0px; padding-bottom:2px; ">                
                <h1 style = "font:serif;color:#060606;font-size:20px; text-align:center;padding-bottom:4px; margin-top:5px; margin-bottom:0px;"><b>Application Information</b></h1>               
            </div>
            <table style = "table-layout: fixed; width: 100%;  border: 1px solid black;border-collapse: collapse;">
                <apex:repeat value="{!fieldsInfo['application']}"  var="f">
                    <tr style="border:1px solid black;">
                        <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                            <b><apex:outputText value="{!f.label1}" /></b>
                           
                        </td>
                        <td style="width: 25%;word-break: break-all;border: 1px solid black; padding-left: 10px;">
                            <!-- <apex:outputText value="{!application[f.apiName1]}" rendered="{!f.label1 == 'Applying To'}" /> -->
                            <apex:outputText value="{!primaryAcademicProgram}" rendered="{!f.label1 == 'Applying To'}" />
                            <apex:outputText value="{!applicationNumber}" rendered="{!f.label1 == 'Application Number'}" />
                            <apex:outputText value="{!applyCET}" rendered="{!f.label1 == 'Applying For REVA CET?'}"/>    
                            <apex:outputText value="{!examType}" rendered="{!f.label1 == 'Exam Type'}" /> 
                            <apex:param value="{!application[f.apiName1]}" />
                          
                        </td>
                        <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                            <b><apex:outputText value="{!f.label2}"/></b>
                        </td>
                        <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                            <!-- <apex:outputText value="{!application[f.apiName2]}" /> -->
                            <apex:outputText value="{!application[f.apiName2]}" rendered="{!f.fieldType2 != 'DATE' && f.fieldType2 != 'DATETIME'}"/>
                            <apex:outputText value="{0, Date, dd-MMM-yyyy}" rendered="{!f.fieldType2 == 'DATE' || f.fieldType2 == 'DATETIME'}">
                                <apex:param value="{!application[f.apiName2]}" />
                            </apex:outputText>
                           
                        </td>
                        
                    </tr> 
                </apex:repeat>
            </table>
            
            <table>
                <div class="header" style="margin:5px; padding-left:10px; color:white; padding-top:0px; padding-bottom:0px; ">  
                    <h1 style = "color:#060606;font-size:20px; text-align:center; margin-top:5px; margin-bottom:0px;  width:100%;"><b>Applicant Information</b></h1> 
                </div>
            </table>
            
            <div>
                <h1 style = "font-size:18px; margin:10px 0px; padding-left:10px; color:white; background-color:gray; width:98.7%;"><b>Personal Information</b></h1>               
            </div>
            <table style = "table-layout:auto; width: 100%;  border: 1px solid black;border-collapse: collapse;">
                <apex:repeat value="{!fieldsInfo['personalInformation']}"  var="f">
                    <tr style="border:1px solid black;">
                        <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                            <b><apex:outputText value="{!f.label1}" /></b>
                        </td>
                        <td style="width: 25%;word-break: break-all;border: 1px solid black; padding-left: 10px;">
                            <!--  <apex:outputText value="{!applicantEmail}" rendered="{!f.label1 == 'Alert Email'}" /> -->
                            <apex:outputText value="{!applicant[f.apiName1]}" rendered="{!f.fieldType1 != 'DATE' && f.fieldType1 != 'DATETIME'}"/>
                            <apex:outputText value="{0, Date, dd-MMM-yyyy}" rendered="{!f.fieldType1 == 'DATE' || f.fieldType1 == 'DATETIME'}">
                                <apex:param value="{!applicant[f.apiName1]}" />
                            </apex:outputText>
                            
                        </td>
                        
                        <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                            <b><apex:outputText value="{!f.label2}" /></b>
                        </td>
                        <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                            <apex:outputText value="{!applicant[f.apiName2]}" rendered="{!f.fieldType2 != 'DATE' && f.fieldType2 != 'DATETIME'}"/>
                            <apex:outputText value="{0, Date, dd-MMM-yyyy}" rendered="{!f.fieldType2 == 'DATE' || f.fieldType2 == 'DATETIME'}">
                                <apex:param value="{!applicant[f.apiName2]}" />
                            </apex:outputText>
                        </td>
                    </tr> 
                </apex:repeat>
                
            </table>
            
            <apex:outputPanel rendered="{!passportNumber != null}"> 
                <div style="padding-top: 5px;padding-bottom: 5px; ">
                    <h1 style = "font-size:18px;padding-left:10px; margin:10px 0px;color:white; background-color:Gray; width:98.7%;"><b>Passport Information</b></h1>   
                </div>
                
                
                <table style = "table-layout: fixed; width: 100%;  border: 1px solid black;border-collapse: collapse;">
                    <apex:repeat value="{!fieldsInfo['passportInformation']}"  var="f">
                        <tr style="border:1px solid black;">
                            <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                                <b><apex:outputText value="{!f.label1}" /></b>
                            </td>
                            <td style="width: 25%; word-break: break-all;border: 1px solid black;padding-left: 10px;">
                                <!-- <apex:outputText value="{!applicant[f.apiName1]}" /> -->
                                <apex:outputText value="{!applicant[f.apiName1]}" rendered="{!f.fieldType1 != 'DATE' && f.fieldType1 != 'DATETIME'}"/>
                                <apex:outputText value="{0, Date, dd-MMM-yyyy}" rendered="{!f.fieldType1 == 'DATE' || f.fieldType1 == 'DATETIME'}">
                                    <apex:param value="{!applicant[f.apiName1]}" />
                                </apex:outputText>
                            </td>
                            <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                                <b><apex:outputText value="{!f.label2}" /></b>
                            </td>
                            <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                                <!-- <apex:outputText value="{!applicant[f.apiName2]}" /> -->
                                <apex:outputText value="{!applicant[f.apiName2]}" rendered="{!f.fieldType2 != 'DATE' && f.fieldType2 != 'DATETIME'}"/>
                                <apex:outputText value="{0, Date, dd-MMM-yyyy}" rendered="{!f.fieldType2 == 'DATE' || f.fieldType2 == 'DATETIME'}">
                                    <apex:param value="{!applicant[f.apiName2]}" />
                                </apex:outputText>
                            </td>
                        </tr> 
                    </apex:repeat>
                    
                </table>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!cetAdmissionMode != 'UQ'}"> 
                <div style="padding-top: 5px;padding-bottom: 5px; ">
                    <h1 style = "font-size:18px;padding-left:10px; margin:10px 0px;color:white; background-color:Gray; width:98.7%;"><b>CET Information</b></h1>   
                </div>
                
                
                <table style = "table-layout: fixed; width: 100%;  border: 1px solid black;border-collapse: collapse;">
                    <apex:repeat value="{!fieldsInfo['cetInformation']}"  var="f">
                        <tr style="border:1px solid black;">
                            <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                                <b><apex:outputText value="{!f.label1}" /></b>
                            </td>
                            <td style="width: 25%; word-break: break-all;border: 1px solid black;padding-left: 10px;">
                                <apex:outputText value="{!applicant[f.apiName1]}" rendered="{!f.fieldType1 != 'DATE' && f.fieldType1 != 'DATETIME'}"/>
                                <apex:outputText value="{0, Date,dd-mm-YYYY}" rendered="{!f.fieldType1 == 'DATE' || f.fieldType1 == 'DATETIME'}">
                                    <apex:param value="{!applicant[f.apiName1]}" />
                                </apex:outputText>
                                
                                    
                                
                            </td>
                            <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                                <b><apex:outputText value="{!f.label2}" /></b>
                            </td>
                            <td style="width: 25%;word-break: break-all;border: 1px solid black;padding-left: 10px;">
                              <apex:outputText value="{!applicant[f.apiName2]}" rendered="{!f.fieldType2 != 'DATE' && f.fieldType2 != 'DATETIME'}"/>
                                <apex:outputText value="{0, Date,dd-MMM-yyyy}" rendered="{!f.fieldType2 == 'DATE' || f.fieldType2 == 'DATETIME'}">
                                    <apex:param value="{!applicant[f.apiName2]}" />
                                </apex:outputText>
                             </td>
                            
                        </tr> 
                        
                    </apex:repeat>
                    
                </table>
            </apex:outputPanel>
            
            <table>
                <div class="header" style="margin:5px; padding-left:10px; padding-top:0px; padding-bottom:0px; ">  
                    <h1 style = "color:#060606;font-size:20px; text-align:center;  margin-bottom:0px;  width:100%;"><b>Parent And Guardian Information</b></h1> 
                </div>
            </table>
            <apex:outputPanel rendered="{!father != null}">
