                            <td style="width: 25%;word-break: break-all;border:1px solid black;padding-left: 10px;">
                                <b><apex:outputText value="{!f.label2}" /></b>
                            </td>
                            <td style="width: 25%;word-break: break-all;border:1px solid black;padding-left: 10px;">
                                <apex:outputText value="{!guardian[f.apiName2]}" />
                            </td>
                        </tr> 
                    </apex:repeat>
                    
                </table>
            </apex:outputPanel>
            
            <table cellpadding="10px" style="margin-top:20px;">
                <div class="header" style=" margin-top:50px margin-bottom:2px; padding-left:10px; padding-top:0px; padding-bottom:0px; ">  
                    <h1 style = "color:#060606; font-size:20px; text-align:center; margin-top:5px; margin-bottom:0px;  width:100%;"><b>Address Information</b></h1> 
                </div>
            </table>
            <apex:outputPanel rendered="{!permanentAddress != null}">
                <h1 style = "font-size:18px; margin:10px 0px; padding-left:10px; color:white; background-color:Gray; width:98.7%;"><b>Permanent Address</b></h1>            
                <table style = "table-layout: fixed; width: 100%;  border: 1px solid black;border-collapse: collapse;">
                    <apex:repeat value="{!fieldsInfo['addressInformation']}"  var="f">
                        <tr style="border:1px solid black;">
                            <td style="width: 25%;word-break: break-all;  border: 1px solid black;border-collapse: collapse;padding-left: 10px;">
                                <b><apex:outputText value="{!f.label1}" /></b>
