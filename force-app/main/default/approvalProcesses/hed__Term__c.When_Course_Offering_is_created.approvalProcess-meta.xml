        <field>Approval_Status__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>School_Head__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Approval from School Head</label>
        <name>Approval_from_School_Head</name>
    </approvalStep>
    <emailTemplate>All/Email_template</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>hed__Term__c.All_Courses_Approved__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>hed__Term__c.Approval_Status__c</field>
            <operation>notEqual</operation>
            <value>Confirmed</value>
        </criteriaItems>
    </entryCriteria>
