<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Student_Offboarded__c</fullName>
    <formula>OR(ISPICKVAL(Student_Status__c, &apos;Completed&apos;),
			ISPICKVAL(Student_Status__c, &apos;Admission Cancelled&apos;),
			ISPICKVAL(Student_Status__c, &apos;Not Reported&apos;))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Student Offboarded</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
