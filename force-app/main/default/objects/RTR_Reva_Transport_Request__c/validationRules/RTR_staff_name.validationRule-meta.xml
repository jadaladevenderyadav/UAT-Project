<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RTR_staff_name</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
  NOT(ISBLANK( RTR_Staff_Name__c )),
  NOT(REGEX(RTR_Staff_Name__c, &quot;^[a-zA-Z .]+$&quot;))
)</errorConditionFormula>
    <errorMessage>Only alphabetic characters are allowed in this field.</errorMessage>
</ValidationRule>
