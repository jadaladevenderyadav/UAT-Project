<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expected_Start_Date_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>DATEVALUE(RTR_Expected_Start_Date_Time__c) &lt; Today()</errorConditionFormula>
    <errorDisplayField>RTR_Expected_Start_Date_Time__c</errorDisplayField>
    <errorMessage>Expected start date must be today or later.</errorMessage>
</ValidationRule>
