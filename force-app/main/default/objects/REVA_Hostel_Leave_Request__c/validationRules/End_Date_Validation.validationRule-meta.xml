<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(Start_Date_and_Time__c)),
    NOT(ISBLANK(End_Date_and_Time__c)),
   End_Date_and_Time__c &lt; Start_Date_and_Time__c
)</errorConditionFormula>
    <errorMessage>End date and time should be equal or greater than start date and time</errorMessage>
</ValidationRule>
